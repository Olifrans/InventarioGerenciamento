@model IEnumerable<InventarioGerenciamento.Web.Models.DAL.funcao>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.f_Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.f_Status)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.f_Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.f_Status)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.f_Id }) |
